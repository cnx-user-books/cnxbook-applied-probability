            </m:msub>
            <m:mo>+</m:mo>
            <m:msub>
              <m:mi>I</m:mi>
              <m:msub>
                <m:mi>C</m:mi>
                <m:mn>10</m:mn>
              </m:msub>
            </m:msub>
          </m:mrow>
        </m:math>
      </equation>
      <para id="id331556">with respective probabilities</para>
      <equation id="id331562">
        <m:math overflow="scroll" mode="display">
          <m:mrow>
            <m:mi>P</m:mi>
            <m:mo>(</m:mo>
            <m:msub>
              <m:mi>C</m:mi>
              <m:mi>i</m:mi>
            </m:msub>
            <m:mo>)</m:mo>
            <m:mo>=</m:mo>
            <m:mn>0</m:mn>
            <m:mo>.</m:mo>
            <m:mn>08</m:mn>
