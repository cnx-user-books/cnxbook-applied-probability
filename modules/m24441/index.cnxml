<exercise id="fs-id18975229" print-placement="end">
<problem id="fs-id18975232">
<para id="id258990">Use the result of <link target-id="fs-id4113105"/> and properties <link document="m24001" target-id="uid9a">(CE9a)</link> and <link document="m24001" target-id="fs-id7541837">(CE10)</link> to show
that</para>
    <equation id="id258994">
      <m:math overflow="scroll" mode="display">
        <m:mrow>
          <m:mi>E</m:mi>
          <m:mrow>
            <m:mo>[</m:mo>
            <m:mi>g</m:mi>
            <m:mrow>
              <m:mo>(</m:mo>
              <m:mi>X</m:mi>
              <m:mo>,</m:mo>
              <m:mi>Y</m:mi>
              <m:mo>)</m:mo>
            </m:mrow>
            <m:mo>|</m:mo>
            <m:mi>Z</m:mi>
            <m:mo>=</m:mo>
            <m:mi>v</m:mi>
            <m:mo>]</m:mo>
          </m:mrow>
          <m:mo>=</m:mo>
          <m:mo>∫</m:mo>
          <m:mi>E</m:mi>
          <m:mrow>
            <m:mo>[</m:mo>
            <m:mi>g</m:mi>
            <m:mrow>
              <m:mo>(</m:mo>
              <m:mi>t</m:mi>
              <m:mo>,</m:mo>
              <m:mi>Y</m:mi>
              <m:mo>)</m:mo>
            </m:mrow>
            <m:mo>|</m:mo>
            <m:mi>X</m:mi>
            <m:mo>=</m:mo>
            <m:mi>t</m:mi>
            <m:mo>,</m:mo>
            <m:mi>Z</m:mi>
            <m:mo>=</m:mo>
            <m:mi>v</m:mi>
