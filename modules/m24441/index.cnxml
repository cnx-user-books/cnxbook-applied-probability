            </m:mrow>
          </m:msup>
          <m:mspace width="0.166667em"/>
          <m:mi>d</m:mi>
          <m:mi>u</m:mi>
          <m:mo>=</m:mo>
          <m:mn>1</m:mn>
          <m:mo>-</m:mo>
