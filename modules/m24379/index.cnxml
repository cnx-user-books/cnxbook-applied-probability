          <m:mrow>
            <m:mo>[</m:mo>
            <m:mi>X</m:mi>
            <m:mi>Y</m:mi>
            <m:mo>]</m:mo>
          </m:mrow>
          <m:mo>=</m:mo>
          <m:mn>1</m:mn>
        </m:mrow>
