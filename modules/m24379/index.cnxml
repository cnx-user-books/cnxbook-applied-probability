          <m:mspace width="0.277778em"/>
          <m:mspace width="0.277778em"/>
          <m:mspace width="0.277778em"/>
          <m:mspace width="0.277778em"/>
          <m:mspace width="0.277778em"/>
          <m:mi>E</m:mi>
          <m:mrow>
            <m:mo>[</m:mo>
            <m:mi>Y</m:mi>
            <m:mo>]</m:mo>
          </m:mrow>
          <m:mo>=</m:mo>
          <m:mn>4</m:mn>
          <m:mo>,</m:mo>
          <m:mspace width="0.277778em"/>
          <m:mspace width="0.277778em"/>
          <m:mspace width="0.277778em"/>
          <m:mspace width="0.277778em"/>
          <m:mspace width="0.277778em"/>
          <m:mi>E</m:mi>
          <m:mrow>
            <m:mo>[</m:mo>
            <m:mi>X</m:mi>
            <m:mi>Y</m:mi>
            <m:mo>]</m:mo>
          </m:mrow>
          <m:mo>=</m:mo>
          <m:mn>15</m:mn>
          <m:mo>,</m:mo>
          <m:mspace width="0.277778em"/>
          <m:mspace width="0.277778em"/>
          <m:mspace width="0.277778em"/>
          <m:mspace width="0.277778em"/>
          <m:mspace width="0.277778em"/>
          <m:mi>E</m:mi>
          <m:mrow>
            <m:mo>[</m:mo>
            <m:msup>
              <m:mi>X</m:mi>
              <m:mn>2</m:mn>
            </m:msup>
            <m:mo>]</m:mo>
          </m:mrow>
          <m:mo>=</m:mo>
          <m:mn>11</m:mn>
          <m:mo>,</m:mo>
          <m:mspace width="0.277778em"/>
          <m:mspace width="0.277778em"/>
          <m:mspace width="0.277778em"/>
          <m:mspace width="0.277778em"/>
          <m:mspace width="0.277778em"/>
          <m:mi> Var </m:mi>
          <m:mspace width="0.166667em"/>
          <m:mrow>
            <m:mo>[</m:mo>
            <m:mi>Y</m:mi>
            <m:mo>]</m:mo>
          </m:mrow>
          <m:mo>=</m:mo>
          <m:mn>5</m:mn>
        </m:mrow>
      </m:math>
    </equation>
    <para id="id137667">Determine <m:math overflow="scroll"><m:mrow><m:mi> Var </m:mi><m:mspace width="0.166667em"/><m:mo>[</m:mo><m:mn>3</m:mn><m:mi>X</m:mi><m:mo>-</m:mo><m:mn>2</m:mn><m:mi>Y</m:mi><m:mo>]</m:mo></m:mrow></m:math>.</para>
</problem>
<solution id="fs-id9336493">
    <code id="id210924" display="block">EX = 3;
EY = 4;
EXY = 15;
EX2 = 11;
VY = 5;
VX = EX2 - EX^2
VX =  2
CV = EXY - EX*EY
CV =  3
VZ = 9*VX + 4*VY - 6*2*CV
VZ =  2
</code>
    <para id="id211024"/>
   </solution>
</exercise>




<exercise id="fs-id10148783" print-placement="end"><problem id="fs-id10712398">
    <para id="id137701">The class <m:math overflow="scroll"><m:mrow><m:mo>{</m:mo><m:mi>A</m:mi><m:mo>,</m:mo><m:mi>B</m:mi><m:mo>,</m:mo><m:mi>C</m:mi><m:mo>,</m:mo><m:mi>D</m:mi><m:mo>,</m:mo><m:mi>E</m:mi><m:mo>,</m:mo><m:mi>F</m:mi><m:mo>}</m:mo></m:mrow></m:math> is independent,
with respective probabilities</para>
