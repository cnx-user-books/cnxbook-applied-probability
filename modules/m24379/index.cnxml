          <m:mi>E</m:mi>
          <m:mo>[</m:mo>
          <m:mi>Y</m:mi>
          <m:mo>]</m:mo>
          <m:mo>=</m:mo>
          <m:mn>1</m:mn>
          <m:mo>,</m:mo>
          <m:mspace width="0.277778em"/>
          <m:mspace width="0.277778em"/>
          <m:mspace width="0.277778em"/>
          <m:mi> Var </m:mi>
          <m:mspace width="0.166667em"/>
          <m:mo>[</m:mo>
          <m:mi>X</m:mi>
          <m:mo>]</m:mo>
          <m:mo>=</m:mo>
          <m:mn>6</m:mn>
          <m:mo>,</m:mo>
          <m:mspace width="0.277778em"/>
          <m:mspace width="0.277778em"/>
          <m:mi> Var </m:mi>
          <m:mspace width="0.166667em"/>
          <m:mo>[</m:mo>
          <m:mi>Y</m:mi>
          <m:mo>]</m:mo>
          <m:mo>=</m:mo>
          <m:mn>4</m:mn>
        </m:mrow>
      </m:math>
    </equation>
    <para id="id138913">Let <m:math overflow="scroll"><m:mrow><m:mi>Z</m:mi><m:mo>=</m:mo><m:mn>2</m:mn><m:msup><m:mi>X</m:mi><m:mn>2</m:mn></m:msup><m:mo>+</m:mo><m:mi>X</m:mi><m:msup><m:mi>Y</m:mi><m:mn>2</m:mn></m:msup><m:mo>-</m:mo><m:mn>3</m:mn><m:mi>Y</m:mi><m:mo>+</m:mo><m:mn>4.</m:mn></m:mrow></m:math>.<newline/>  Determine <m:math overflow="scroll"><m:mrow><m:mi>E</m:mi><m:mo>[</m:mo><m:mi>Z</m:mi><m:mo>].</m:mo></m:mrow></m:math></para>
   </problem>
<solution id="fs-id7279092"> 
    <code id="id212142" display="block">EX = 2;
EY = 1;
VX = 6;
VY = 4;
EX2 = VX + EX^2
EX2 =  10
EY2 = VY + EY^2
EY2 =   5
EZ = 2*EX2 + EX*EY2 - 3*EY + 4
EZ =   31
</code>
</solution>
