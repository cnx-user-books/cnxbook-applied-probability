          <m:mn>0</m:mn>
          <m:mo>.</m:mo>
          <m:mn>80</m:mn>
          <m:mspace width="0.277778em"/>
          <m:mspace width="0.277778em"/>
          <m:mspace width="0.277778em"/>
          <m:mi>P</m:mi>
          <m:mrow>
            <m:mo>(</m:mo>
            <m:mi>H</m:mi>
