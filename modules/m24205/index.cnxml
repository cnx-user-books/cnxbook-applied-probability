              <m:mi>S</m:mi>
              <m:mo>)</m:mo>
            </m:mrow>
            <m:mrow>
              <m:mi>P</m:mi>
              <m:mo>(</m:mo>
              <m:msup>
                <m:mi>H</m:mi>
                <m:mi>c</m:mi>
              </m:msup>
              <m:mo>|</m:mo>
              <m:mi>S</m:mi>
              <m:mo>)</m:mo>
            </m:mrow>
          </m:mfrac>
          <m:mo>=</m:mo>
          <m:mfrac>
            <m:mrow>
              <m:mi>P</m:mi>
              <m:mo>(</m:mo>
              <m:mi>H</m:mi>
              <m:mo>)</m:mo>
              <m:mi>P</m:mi>
              <m:mo>(</m:mo>
              <m:mi>S</m:mi>
              <m:mo>|</m:mo>
              <m:mi>H</m:mi>
              <m:mo>)</m:mo>
            </m:mrow>
            <m:mrow>
              <m:mi>P</m:mi>
              <m:mrow>
                <m:mo>(</m:mo>
                <m:msup>
                  <m:mi>H</m:mi>
                  <m:mi>c</m:mi>
                </m:msup>
                <m:mo>)</m:mo>
              </m:mrow>
              <m:mi>P</m:mi>
              <m:mrow>
                <m:mo>(</m:mo>
                <m:mi>S</m:mi>
                <m:mo>|</m:mo>
                <m:msup>
                  <m:mi>H</m:mi>
                  <m:mi>c</m:mi>
                </m:msup>
                <m:mo>)</m:mo>
              </m:mrow>
            </m:mrow>
          </m:mfrac>
        </m:mrow>
      </m:math>
    </equation>
    <equation id="id197606">
      <m:math overflow="scroll" mode="display">
        <m:mrow>
          <m:mi>P</m:mi>
          <m:mrow>
            <m:mo>(</m:mo>
            <m:mi>S</m:mi>
            <m:mo>)</m:mo>
          </m:mrow>
          <m:mo>=</m:mo>
          <m:mi>P</m:mi>
