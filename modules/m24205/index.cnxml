            <m:msup>
              <m:mi>S</m:mi>
              <m:mi>c</m:mi>
            </m:msup>
            <m:mo>)</m:mo>
          </m:mrow>
          <m:mo>=</m:mo>
          <m:mn>0</m:mn>
