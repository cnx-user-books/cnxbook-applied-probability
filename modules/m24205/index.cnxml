            </m:msup>
            <m:mo>)</m:mo>
          </m:mrow>
          <m:mspace width="0.277778em"/>
          <m:mspace width="0.277778em"/>
          <m:mtext>which</m:mtext>
          <m:mspace width="4.pt"/>
          <m:mtext>implies</m:mtext>
        </m:mrow>
      </m:math>
    </equation>
    <equation id="id197704">
      <m:math overflow="scroll" mode="display">
