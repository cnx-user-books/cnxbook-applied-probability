              </m:mrow>
            </m:mrow>
          </m:mfrac>
          <m:mo>=</m:mo>
          <m:mn>5</m:mn>
          <m:mo>/</m:mo>
          <m:mn>7</m:mn>
          <m:mspace width="0.277778em"/>
