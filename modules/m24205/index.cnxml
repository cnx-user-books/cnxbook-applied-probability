              <m:mo>-</m:mo>
              <m:mi>P</m:mi>
              <m:mrow>
                <m:mo>(</m:mo>
                <m:mi>S</m:mi>
                <m:mo>|</m:mo>
                <m:msup>
                  <m:mi>H</m:mi>
