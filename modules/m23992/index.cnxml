        <item id="uid2">
          
          <emphasis effect="bold">Simple random variable</emphasis>
          <m:math overflow="scroll">
            <m:mstyle scriptlevel="0" displaystyle="true">
              <m:mrow>
                <m:mi>X</m:mi>
                <m:mo>=</m:mo>
                <m:munderover>
                  <m:mo>∑</m:mo>
                  <m:mrow>
                    <m:mi>i</m:mi>
                    <m:mo>=</m:mo>
                    <m:mn>1</m:mn>
                  </m:mrow>
                  <m:mi>n</m:mi>
                </m:munderover>
                <m:msub>
                  <m:mi>t</m:mi>
                  <m:mi>i</m:mi>
                </m:msub>
                <m:msub>
                  <m:mi>I</m:mi>
                  <m:msub>
                    <m:mi>A</m:mi>
                    <m:mi>i</m:mi>
                  </m:msub>
                </m:msub>
                <m:mspace width="0.277778em"/>
                <m:mspace width="0.277778em"/>
                <m:mtext>(a</m:mtext>
                <m:mspace width="4.pt"/>
                <m:mtext>primitive</m:mtext>
                <m:mspace width="4.pt"/>
                <m:mtext>form)</m:mtext>
                <m:mspace width="0.277778em"/>
                <m:mspace width="0.277778em"/>
                <m:mi>P</m:mi>
                <m:mrow>
                  <m:mo>(</m:mo>
                  <m:msub>
                    <m:mi>A</m:mi>
                    <m:mi>i</m:mi>
                  </m:msub>
                  <m:mo>)</m:mo>
                </m:mrow>
                <m:mo>=</m:mo>
                <m:msub>
                  <m:mi>p</m:mi>
                  <m:mi>i</m:mi>
                </m:msub>
              </m:mrow>
            </m:mstyle>
          </m:math>
          <equation id="id90729"><m:math overflow="scroll" mode="display">
              <m:mrow>
                <m:mi>E</m:mi>
                <m:mrow>
                  <m:mo>[</m:mo>
                  <m:mi>X</m:mi>
                  <m:mo>]</m:mo>
                </m:mrow>
                <m:mo>=</m:mo>
                <m:munderover>
                  <m:mrow><m:mo>∑</m:mo></m:mrow>
                  <m:mrow>
                    <m:mi>i</m:mi>
                    <m:mo>=</m:mo>
                    <m:mn>1</m:mn>
                  </m:mrow>
                  <m:mi>n</m:mi>
                </m:munderover>
                <m:msub>
                  <m:mi>t</m:mi>
                  <m:mi>i</m:mi>
                </m:msub>
                <m:msub>
                  <m:mi>p</m:mi>
                  <m:mi>i</m:mi>
                </m:msub>
                <m:mspace width="0.277778em"/>
                <m:mspace width="0.277778em"/>
                <m:mspace width="0.277778em"/>
                <m:mi> Var </m:mi>
                <m:mspace width="0.166667em"/>
                <m:mrow>
                  <m:mo>[</m:mo>
                  <m:mi>X</m:mi>
                  <m:mo>]</m:mo>
                </m:mrow>
                <m:mo>=</m:mo>
                <m:munderover>
                  <m:mrow><m:mo>∑</m:mo></m:mrow>
                  <m:mrow>
                    <m:mi>i</m:mi>
                    <m:mo>=</m:mo>
                    <m:mn>1</m:mn>
                  </m:mrow>
                  <m:mi>n</m:mi>
                </m:munderover>
                <m:msubsup>
                  <m:mi>t</m:mi>
                  <m:mi>i</m:mi>
                  <m:mn>2</m:mn>
                </m:msubsup>
                <m:msub>
                  <m:mi>p</m:mi>
                  <m:mi>i</m:mi>
                </m:msub>
                <m:msub>
                  <m:mi>q</m:mi>
                  <m:mi>i</m:mi>
                </m:msub>
                <m:mo>-</m:mo>
                <m:mn>2</m:mn>
                <m:munder>
                  <m:mo>∑</m:mo>
                  <m:mrow>
                    <m:mi>i</m:mi>
                    <m:mo>&lt;</m:mo>
                    <m:mi>j</m:mi>
                  </m:mrow>
                </m:munder>
                <m:msub>
                  <m:mi>t</m:mi>
                  <m:mi>i</m:mi>
                </m:msub>
                <m:msub>
                  <m:mi>t</m:mi>
                  <m:mi>j</m:mi>
                </m:msub>
                <m:msub>
                  <m:mi>p</m:mi>
                  <m:mi>i</m:mi>
                </m:msub>
                <m:msub>
                  <m:mi>p</m:mi>
                  <m:mi>j</m:mi>
                </m:msub>
                <m:mspace width="0.277778em"/>
                <m:mspace width="0.277778em"/>
                <m:mspace width="0.277778em"/>
                <m:mspace width="0.277778em"/>
                <m:msub>
                  <m:mi>M</m:mi>
                  <m:mi>X</m:mi>
                </m:msub>
                <m:mrow>
                  <m:mo>(</m:mo>
                  <m:mi>s</m:mi>
                  <m:mo>)</m:mo>
                </m:mrow>
                <m:mo>=</m:mo>
                <m:munderover>
                  <m:mrow><m:mo>∑</m:mo></m:mrow>
                  <m:mrow>
                    <m:mi>i</m:mi>
                    <m:mo>=</m:mo>
                    <m:mn>1</m:mn>
                  </m:mrow>
                  <m:mi>n</m:mi>
                </m:munderover>
                <m:msub>
                  <m:mi>p</m:mi>
                  <m:mi>i</m:mi>
                </m:msub>
                <m:msup>
                  <m:mi>e</m:mi>
                  <m:mrow>
                    <m:mi>s</m:mi>
                    <m:msub>
                      <m:mi>t</m:mi>
                      <m:mi>i</m:mi>
                    </m:msub>
                  </m:mrow>
                </m:msup>
              </m:mrow>
            </m:math>
          </equation>
