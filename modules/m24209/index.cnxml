          <m:mspace width="0.277778em"/>
          <m:mtext>poisson</m:mtext>
          <m:mo>(</m:mo>
          <m:mn>3</m:mn>
          <m:mo>·</m:mo>
          <m:mn>2</m:mn>
          <m:mo>=</m:mo>
