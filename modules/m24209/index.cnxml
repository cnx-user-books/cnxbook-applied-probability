          <m:mspace width="0.277778em"/>
          <m:mspace width="0.277778em"/>
          <m:mi>Y</m:mi>
          <m:mo>∼</m:mo>
          <m:mspace width="0.277778em"/>
          <m:mtext>poisson</m:mtext>
          <m:mo>(</m:mo>
          <m:mn>1</m:mn>
          <m:mo>/</m:mo>
          <m:mn>3</m:mn>
          <m:mo>·</m:mo>
          <m:mn>20</m:mn>
          <m:mo>)</m:mo>
        </m:mrow>
      </m:math>
    </equation>
