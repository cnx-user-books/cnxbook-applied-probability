              <m:mi>E</m:mi>
              <m:mn>4</m:mn>
              <m:mi>c</m:mi>
            </m:msubsup>
            <m:mo>⋁</m:mo>
            <m:msub>
              <m:mi>E</m:mi>
              <m:mn>1</m:mn>
            </m:msub>
