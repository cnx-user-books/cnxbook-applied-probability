    <para id="id269388">Substituting values, we get</para>
    <equation id="id269393">
      <m:math overflow="scroll" mode="display">
        <m:mrow>
          <m:mfrac>
            <m:mrow>
              <m:mi>P</m:mi>
              <m:mo>(</m:mo>
              <m:mi>H</m:mi>
              <m:mo>|</m:mo>
              <m:msub>
                <m:mi>E</m:mi>
                <m:mn>1</m:mn>
              </m:msub>
              <m:msub>
                <m:mi>E</m:mi>
                <m:mn>2</m:mn>
              </m:msub>
              <m:mo>)</m:mo>
            </m:mrow>
            <m:mrow>
              <m:mi>P</m:mi>
              <m:mo>(</m:mo>
              <m:msup>
                <m:mi>H</m:mi>
                <m:mi>c</m:mi>
              </m:msup>
              <m:mo>|</m:mo>
              <m:msub>
                <m:mi>E</m:mi>
                <m:mn>1</m:mn>
              </m:msub>
              <m:msub>
                <m:mi>E</m:mi>
