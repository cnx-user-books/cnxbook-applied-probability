                  <m:mi>H</m:mi>
                  <m:mi>c</m:mi>
                </m:msup>
                <m:mo>)</m:mo>
              </m:mrow>
            </m:mrow>
            <m:mrow>
              <m:mi>P</m:mi>
              <m:mrow>
                <m:mo>(</m:mo>
                <m:mi>S</m:mi>
                <m:mo>|</m:mo>
                <m:mi>H</m:mi>
                <m:mo>)</m:mo>
              </m:mrow>
              <m:mo>-</m:mo>
              <m:mi>P</m:mi>
              <m:mrow>
                <m:mo>(</m:mo>
                <m:mi>S</m:mi>
                <m:mo>|</m:mo>
                <m:msup>
                  <m:mi>H</m:mi>
                  <m:mi>c</m:mi>
                </m:msup>
                <m:mo>)</m:mo>
              </m:mrow>
            </m:mrow>
          </m:mfrac>
          <m:mo>=</m:mo>
          <m:mfrac>
            <m:mrow>
              <m:mn>0</m:mn>
              <m:mo>.</m:mo>
              <m:mn>75</m:mn>
              <m:mo>-</m:mo>
              <m:mn>0</m:mn>
              <m:mo>.</m:mo>
              <m:mn>20</m:mn>
            </m:mrow>
            <m:mrow>
              <m:mn>0</m:mn>
              <m:mo>.</m:mo>
              <m:mn>92</m:mn>
              <m:mo>-</m:mo>
              <m:mn>0</m:mn>
