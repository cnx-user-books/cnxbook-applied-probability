                <m:mi>E</m:mi>
                <m:mi>c</m:mi>
              </m:msup>
              <m:mo>)</m:mo>
            </m:mrow>
          </m:mfrac>
          <m:mo>=</m:mo>
          <m:mfrac>
            <m:mrow>
              <m:mo>(</m:mo>
              <m:mn>11</m:mn>
              <m:mo>/</m:mo>
              <m:mn>45</m:mn>
              <m:mo>)</m:mo>
