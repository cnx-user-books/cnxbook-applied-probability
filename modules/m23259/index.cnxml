              </m:msub>
              <m:mo>)</m:mo>
            </m:mrow>
          </m:mfrac>
          <m:mo>=</m:mo>
          <m:mn>2</m:mn>
          <m:mo>·</m:mo>
          <m:mfrac>
            <m:mrow>
              <m:mn>0</m:mn>
              <m:mo>.</m:mo>
              <m:mn>95</m:mn>
              <m:mo>·</m:mo>
              <m:mn>0</m:mn>
              <m:mo>.</m:mo>
              <m:mn>92</m:mn>
            </m:mrow>
            <m:mrow>
              <m:mn>0</m:mn>
              <m:mo>.</m:mo>
              <m:mn>10</m:mn>
              <m:mo>·</m:mo>
              <m:mn>0</m:mn>
              <m:mo>.</m:mo>
              <m:mn>05</m:mn>
            </m:mrow>
          </m:mfrac>
          <m:mo>=</m:mo>
          <m:mfrac>
            <m:mn>1748</m:mn>
            <m:mn>5</m:mn>
          </m:mfrac>
          <m:mspace width="0.277778em"/>
          <m:mspace width="0.277778em"/>
          <m:mspace width="0.277778em"/>
          <m:mtext>so</m:mtext>
          <m:mspace width="4.pt"/>
          <m:mtext>that</m:mtext>
