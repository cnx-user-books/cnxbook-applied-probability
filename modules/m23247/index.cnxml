                  <m:mrow>
                    <m:mn>0001</m:mn>
                    <m:mo>∼</m:mo>
                    <m:mn>1</m:mn>
                  </m:mrow>
                </m:math>
              </entry>
              <entry>
                <m:math overflow="scroll">
                  <m:mrow>
                    <m:mn>0101</m:mn>
                    <m:mo>∼</m:mo>
                    <m:mn>5</m:mn>
                  </m:mrow>
                </m:math>
              </entry>
              <entry>
                <m:math overflow="scroll">
                  <m:mrow>
                    <m:mn>1001</m:mn>
                    <m:mo>∼</m:mo>
                    <m:mn>9</m:mn>
                  </m:mrow>
                </m:math>
              </entry>
              <entry>
                <m:math overflow="scroll">
                  <m:mrow>
                    <m:mn>1101</m:mn>
                    <m:mo>∼</m:mo>
                    <m:mn>13</m:mn>
                  </m:mrow>
                </m:math>
              </entry>
            </row>
            <row>
              <entry>
                <m:math overflow="scroll">
                  <m:mrow>
                    <m:mn>0010</m:mn>
                    <m:mo>∼</m:mo>
                    <m:mn>2</m:mn>
                  </m:mrow>
                </m:math>
              </entry>
              <entry>
                <m:math overflow="scroll">
                  <m:mrow>
                    <m:mn>0110</m:mn>
                    <m:mo>∼</m:mo>
                    <m:mn>6</m:mn>
                  </m:mrow>
                </m:math>
              </entry>
              <entry>
                <m:math overflow="scroll">
                  <m:mrow>
                    <m:mn>1010</m:mn>
                    <m:mo>∼</m:mo>
                    <m:mn>10</m:mn>
                  </m:mrow>
                </m:math>
              </entry>
              <entry>
                <m:math overflow="scroll">
                  <m:mrow>
                    <m:mn>1110</m:mn>
                    <m:mo>∼</m:mo>
                    <m:mn>14</m:mn>
                  </m:mrow>
                </m:math>
              </entry>
            </row>
            <row>
              <entry>
                <m:math overflow="scroll">
                  <m:mrow>
                    <m:mn>0011</m:mn>
                    <m:mo>∼</m:mo>
                    <m:mn>3</m:mn>
                  </m:mrow>
                </m:math>
              </entry>
              <entry>
                <m:math overflow="scroll">
                  <m:mrow>
                    <m:mn>0111</m:mn>
                    <m:mo>∼</m:mo>
                    <m:mn>7</m:mn>
                  </m:mrow>
                </m:math>
              </entry>
              <entry>
                <m:math overflow="scroll">
                  <m:mrow>
                    <m:mn>1011</m:mn>
                    <m:mo>∼</m:mo>
                    <m:mn>11</m:mn>
                  </m:mrow>
                </m:math>
              </entry>
              <entry>
                <m:math overflow="scroll">
                  <m:mrow>
