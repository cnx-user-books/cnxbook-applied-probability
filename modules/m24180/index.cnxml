              <m:mi>n</m:mi>
              <m:mo>-</m:mo>
              <m:mn>1</m:mn>
            </m:mrow>
          </m:mfrac>
          <m:mo>+</m:mo>
          <m:mfrac>
            <m:mi>r</m:mi>
            <m:mi>n</m:mi>
          </m:mfrac>
          <m:mo>•</m:mo>
          <m:mfrac>
            <m:mrow>
              <m:mi>n</m:mi>
              <m:mo>-</m:mo>
              <m:mi>r</m:mi>
            </m:mrow>
            <m:mrow>
              <m:mi>n</m:mi>
              <m:mo>-</m:mo>
              <m:mn>1</m:mn>
            </m:mrow>
          </m:mfrac>
          <m:mo>+</m:mo>
          <m:mfrac>
            <m:mrow>
              <m:mi>n</m:mi>
              <m:mo>-</m:mo>
              <m:mi>r</m:mi>
            </m:mrow>
            <m:mi>n</m:mi>
          </m:mfrac>
          <m:mo>•</m:mo>
          <m:mfrac>
            <m:mi>r</m:mi>
